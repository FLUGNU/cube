function Perso_Key_Check() {
	press_left = keyboard_check(vk_left);
	press_right = keyboard_check(vk_right);
	press_up = keyboard_check_pressed(vk_up);
	press_up_held = keyboard_check(vk_up);
	press_up_released = keyboard_check_released(vk_up);
	press_down = keyboard_check(vk_down);
	onpress=(keyboard_check_pressed(vk_right)or keyboard_check_pressed(vk_left))


	// Commandes time
	if onpress{t=current_time}




}
